<?xml version="1.0" encoding="UTF-8"?>
<RecipeDefs>

<!-- Fingers: Modified from DE Surgeries (GE) -->
<RecipeDef ParentName="SurgerySimple">
  <defName>ReplaceMissingFinger</defName>
  <label>replace missing finger</label>
  <description>Replace a missing finger.</description>
  <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
  <jobString>replacing finger</jobString>
  <surgerySuccessChanceFactor>1.42</surgerySuccessChanceFactor>
  <deathOnFailedSurgeryChance>0.01</deathOnFailedSurgeryChance>
        <workAmount>2000</workAmount>
  <ingredients>
    <li>
      <filter>
        <categories>
          <li>Medicine</li>
        </categories>
      </filter>
      <count>2</count>
    </li>
    <li>
      <filter>
        <thingDefs>
          <li>Steel</li>
        </thingDefs>
      </filter>
      <count>5</count>
    </li>
  </ingredients>
  <fixedIngredientFilter>
    <categories>
      <li>Medicine</li>
    </categories>
    <exceptedThingDefs>
      <li>HerbalMedicine</li>
    </exceptedThingDefs>
    <thingDefs>
      <li>Steel</li>
    </thingDefs>
  </fixedIngredientFilter>
  <skillRequirements>
    <li>
     <skill>Medicine</skill>
       <minLevel>6</minLevel>
     </li>
   </skillRequirements>
  <appliedOnFixedBodyParts>
    <li>LeftHandPinky</li>
    <li>LeftHandRingFinger</li>
    <li>LeftHandMiddleFinger</li>
    <li>LeftHandIndexFinger</li>
    <li>LeftHandThumb</li>
    <li>RightHandPinky</li>
    <li>RightHandRingFinger</li>
    <li>RightHandMiddleFinger</li>
    <li>RightHandIndexFinger</li>
    <li>RightHandThumb</li>  
  </appliedOnFixedBodyParts>
   <researchPrerequisite>SimpleProsthetics</researchPrerequisite>
</RecipeDef>

