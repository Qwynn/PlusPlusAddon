	<ThingDef ParentName="FurnitureBase">
		<defName>StoolLinkable</defName>
		<label>linkable stool</label>
		<uiIconPath>Tables/chair_menuicon</uiIconPath>
		<graphicData>
			<texPath>Tables/chairlink</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Custom2</li>
			</linkFlags>
			<shadowData>
				<volume>(0.3, 0.3, 0.6)</volume>
			</shadowData>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
			<Mass>3</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>3</Beauty>
			<Comfort>0.5</Comfort>
		</statBases>
		<comps>
			<li Class="CompProperties_RoomIdentifier">
				<roomStat>Impressiveness</roomStat>
			</li>
		</comps>
		<description>People sit here.</description>
		<size>(1,1)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>15</costStuffCount>
		<pathCost>30</pathCost>
		<designationCategory>Furniture</designationCategory>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
			<building>
				<isSittable>true</isSittable>
			</building>
		<minifiedDef>MinifiedFurniture</minifiedDef>
		<researchPrerequisites>
			<li>PPCompConstruction</li>
		</researchPrerequisites>
	</ThingDef>

